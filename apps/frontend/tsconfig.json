{
	"extends": "../../tsconfig.json",
	"include": ["src"],
	"exclude": ["node_modules"],
	"compilerOptions": {
		"skipLibCheck": true,
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"jsx": "react-jsx",
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"],
		},
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
	},
}
